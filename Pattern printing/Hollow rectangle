#include <stdio.h>
int main() {
    int i, j, m, n;
    printf("Enter the value of rows and columns");
    scanf("%d %d",&m,&n);
    for (i = 1; i <= n; i++)
    {
        for (j = 1; j <= m; j++)
        {
            if (i==1 || i==n || j==1 || j==m)           
                printf("*");           
            else
                printf(" ");           
        }
        printf("\n");
    }
    return 0;
